<% if @employee.errors.any? %>
<p class = "error_message"><%= pluralize(@employee.errors.count, "error") %> prevented this employee from being saved </p>
<ul>
    <% @employee.errors.full_messages.each do |e| %>
        <li class="error_message"><%= e %></li>
    <% end %>
</ul>
<% end %>

<%= form_for @employee do |f| %>
    <%= f.label :first_name %>
    <%= f.text_field :first_name %>
    <%= f.label :last_name %>
    <%= f.text_field :last_name %>
    <%= f.label :alias %>
    <%= f.text_field :alias %>
     <%= f.label :title %>
    <%= f.text_field :title %>
    <%= f.label :office %>
    <%= f.text_field :office %>
    <%= f.label :dog %>
    <%= f.label :picture_url %>
    <%= f.text_field :img_url %>
    <%= f.collection_select :dog_id, @dogs, :id, :name %>
    <%= f.submit %>
<% end %>